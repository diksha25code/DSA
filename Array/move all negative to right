#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int a[]={3,-2,5,-4,1,6};
    int n=6;
    int i=-1;
    vector<int>v;
    vector<int>v1;
    for(int j=0;j<n;j++){
        if(a[j]<0){
           i++;
        v.push_back(a[j]);
         
        }
        else{
            v1.push_back(a[j]);
        }
        
    }
v1.insert(v1.end(),v.begin(),v.end());

for(int i=0;i<n;i++){
    cout<<v1[i]<<" ";
}
    return 0;
}

//3 5 1 6 -2 -4
