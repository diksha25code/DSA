#include <iostream>
#include <climits>
using namespace std;

int main()
{
    int a[]={6, -3, -10, 0, 2};
    int n=5;
    int mn=INT_MIN;
    int p=1;
    
    for(int i=0;i<n;i++){
        for(int j=i;j<n;j++){
            p=p*a[j];
            mn=max(mn,p);
        }
    }
    cout<<mn;
    return 0;
}

//180
