#include <iostream>

using namespace std;

int main()
{
    int a[] = {1,2,4,3,6};
    int n=5;
    int x=10;
    int ans=0;
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            for(int k=j+1;j<n;j++){
                if(a[i]+a[j]+a[k]==x)
                   ans=1;
                   break; 
                
                // else ans=0;
            }
        }
    }

cout<<ans;
    return 0;
}

//1
//tc : o(n^3)
