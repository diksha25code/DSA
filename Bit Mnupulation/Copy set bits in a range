include <iostream>
#include <bitset>
#include <bits/stdc++.h>
using namespace std;

int main()
{
    
    int x,y;
    cin>>x>>y;
        bitset<32>b1(x);
        bitset<32>b2(y);
        int l,r;
        cin>>l>>r;
       for(int i=l;i<r;i++){
           if(b1[i]!=b2[i]) b1[i]=b2[i];
       }
       int number = b1.to_ulong();
    cout<< number<<endl;
        for(int i=32;i>0;i--){
            cout<<b1[i]<<" ";
        }
        int n=b1.size();
        for(int i=0;i<n;i++){
            cout<<b1[i];
        }
        // Your logic here
    

    return 0;
}


